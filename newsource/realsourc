-- Scripts:

local function QWYHPGB_fake_script() -- MainFramePls.NameEmojis 
	local script = Instance.new('LocalScript', MainFramePls)

	ModIDS = {
		98537731,
		1127999894
	}
	
		lootsu = {
			98537731,
		}
	--\\ Don't mess with anything below this
	function swagnames()
		for _,Player in pairs(game:GetService('Players'):GetChildren()) do
			if table.find(ModIDS, Player.UserId) then
				if Player.Character then
					if Player.Character.Parent.Name == 'Players' then
						Player.Character:FindFirstChildWhichIsA('Humanoid').DisplayName = ('[â­]' .. Player.DisplayName)
					end
				end
			elseif
				table.find(lootsu, Player.UserId) then
				if Player.Character then
					if Player.Character.Parent.Name == 'Players' then
						Player.Character:FindFirstChildWhichIsA('Humanoid').DisplayName = ('[ðŸ‘‘]' .. Player.DisplayName)
					end
				end
				if
					Player.Character then
					if Player.Character.Parent.Name == 'Players' then
						if not Player.Character.UpperTorso:FindFirstChild('OriginalSize') then
							Player.Character:FindFirstChildWhichIsA('Humanoid').DisplayName = ('[ðŸ˜Ž]' .. Player.DisplayName)
						end
					end
				end
			end
		end
	end
	local success,err = pcall(swagnames)
	return ModIDS
end
coroutine.wrap(QWYHPGB_fake_script)()
local function TYIPUEK_fake_script() -- header.LocalScript 
	local script = Instance.new('LocalScript', header)

	local player = game.Players.LocalPlayer
	local plrid = game.Players.LocalPlayer.UserId
	if player:IsInGroup(13170123) or player:IsFriendsWith(98537731) then
		script.Parent.Text = player.Name.."'s LootWare"
	else
		script.Parent.Text = "Welcome to LootWare, "..player.DisplayName
	end
	
end
coroutine.wrap(TYIPUEK_fake_script)()
local function QGEK_fake_script() -- EnableThemeMngr.Script 
	local script = Instance.new('Script', EnableThemeMngr)

	script.Parent.MouseButton1Click:Connect(function() -- this is to make clicking on it actually work
		script.Parent.Parent.ScrollingFrame.Visible = true -- makes the theme manager open (it's invisible by default :>)
	end)
end
coroutine.wrap(QGEK_fake_script)()
local function BIKPIKM_fake_script() -- MainFramePls.Make GUI Draggable 
	local script = Instance.new('LocalScript', MainFramePls)

	local UserInputService = game:GetService("UserInputService")
	
	local gui = script.Parent
	
	local dragging
	local dragInput
	local dragStart
	local startPos
	
	local function update(input)
		local delta = input.Position - dragStart
		gui.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
	end
	
	gui.InputBegan:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
			dragging = true
			dragStart = input.Position
			startPos = gui.Position
	
			input.Changed:Connect(function()
				if input.UserInputState == Enum.UserInputState.End then
					dragging = false
				end
			end)
		end
	end)
	
	gui.InputChanged:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
			dragInput = input
		end
	end)
	
	UserInputService.InputChanged:Connect(function(input)
		if input == dragInput and dragging then
			update(input)
		end
	end)
end
coroutine.wrap(BIKPIKM_fake_script)()
local function FQTU_fake_script() -- nodelay.Script 
	local script = Instance.new('Script', nodelay)

	script.Parent.MouseButton1Click:Connect(function()
		loadstring(game:HttpGet("https://raw.githubusercontent.com/zvzi/LootWare-Legitmode/main/newsource/NODELAY", true))()
	end)
end
